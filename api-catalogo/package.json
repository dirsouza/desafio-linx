    "@types/jest": "26.0.4",
    "@types/node": "14.0.22",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.1",
    "ts-loader": "8.0.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
